//-
  options.text - текст внутри элемента
  options.marker - маркер справа от элемента
mixin type(options)
  - 
    const { text, marker } = options;
    const modifierClass = `types__type--${marker}`;
  li.types__type(class = modifierClass, class = attributes.class)
    span.types__type-marker= marker
    span.types__type-text= text